
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Razor</title>
    <link rel="stylesheet" asp-href-include="lib/bootstrap/dist/css/*.min.css" />
    <script id="jsonData" type="application/json">
        @Json.Serialize(new string[] { "apple", "orange", "pie"})
    </script>
    <script asp-src-include="lib/jquery/dist/*.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var list = $("#list")
            JSON.parse($("#jsonData").text()).forEach(function (val) {
                console.log("Val: " + val);
                list.append($("<li>").text(val));
            });
        });
    </script>
</head>
<body class="m-1 p-1">
    this the List View
    @Html.Partial("MyPartial")

    @Html.Partial("MyStronglyTypedPartial", new string[] { "apple", "orange", "pear"})
    <ul id="list"></ul>
</body>
</html>
